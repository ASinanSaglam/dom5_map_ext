{
  "publisher": "als251",
  "repository": {
    "type": "git",
    "url": "https://github.com/asinansaglam/dom5_map_ext"
  },
  "name": "dom5map",
  "displayName": "Dominions 5 map extension",
  "description": "Map editor extension for Dominions 5",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
      "Snippets"
  ],
  "activationEvents": [
    "onLanguage:dom5map"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [
      {
          "id": "dom5map",
          "aliases": [
              "Dom5 map language"
          ],
          "extensions": [
              ".map"
          ],
          "configuration": "./dom5map-language-configuration.json"
      }
    ],
    "snippets": [
      {
          "language": "dom5map",
          "path": "./snippets/dom5map-snippets.json"
      }
    ]
  }
}
